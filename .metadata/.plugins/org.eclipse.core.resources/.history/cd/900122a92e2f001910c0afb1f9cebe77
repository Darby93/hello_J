package kr.or.bit;

public class InitTest {
	public static int cv = 1;  //static 변수, class 변수!!  -  클래스 로딩될때  
	public int iv = 1;   //명시적 초기화 
	
    static { cv = 2;} //static 초기화 블럭, class 초기화 블럭    -  클래스 로딩될때  
    { iv = 2;}  // 인스턴스 초기화 블럭 
	
	public InitTest() { //생성자 함수 ( 초기화 목적 : instance variable 초기화)
		inv = 3 ;
	}
	
	
	
	
	
}
