
public class Lotto_01 {

	public static void main(String[] args) {
		int [] lotto = new int[6];
		
		
		for (int i = 0; i < 6; i++) {
			lotto[i] = (int)(Math.random()*45+1);
			for (int j =  0; i > j ; j++) {
				if (lotto[i] == lotto[j]) {
					i--;
					break;
				}
			}
		}
		
		for (int i = 0; i < lotto.length; i ++) {
			for (int j = i+1; j > lotto.length; j++) {
				if(lotto[i] < lotto[j]) {
					int temp = lotto[i];
					lotto[i] = lotto[j];
					lotto[j] = temp;
				}
			}
		}
		
		
		
		
		
		for ( int i = 0; i < 6; i++) {
			System.out.println(lotto[i]);
		}
		
	}

}
