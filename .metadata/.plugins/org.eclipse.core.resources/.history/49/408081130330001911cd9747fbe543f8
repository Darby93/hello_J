package kr.or.bit;

import java.util.Random;
import java.util.Scanner;

public class Lotto {
	private Scanner scanner; // 부품
	private Random r; // 부품
	private int[] numbers;
	
	
	// static{} , {} 초기자
	public Lotto() { // 특수한 목적 : member field 초기화 : (생성자 함수), constructor
		scanner = new Scanner(System.in); // 초기화 (참조 변수는 주소값을 갖는 것)
		r = new Random();
		numbers = new int[6];
	}
	
	
	
	
	
	
}
