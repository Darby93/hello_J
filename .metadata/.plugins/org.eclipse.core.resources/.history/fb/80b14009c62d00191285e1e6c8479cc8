package Group_2;

public class Vacuum {
	public String company;
	public String model;
	public String color;
	public String date;
	public int maxBattery;
	
	public int suctionForce;
	public int restoBattery;
	public int presentSpeed;
	public boolean power;
	
	public void powerOn() {
		power = true;
	}
	
	public void powerOff() {
		power = false;
	}
	
	
	Head head  = new Head();
	Motor motor = new Motor();
	Charger charger = new Charger();
	
}
