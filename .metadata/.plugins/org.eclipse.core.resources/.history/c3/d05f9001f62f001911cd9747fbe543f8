//1 값 입력 >> selectBasicNumber()  함수 호출 (실번호 추출) >> 출력
//2 값 입력 >> 프로그램 종료

public class Lotto {
	private Scanner scanner; //부품
	private Random r; //부품
	private int [] numbers;
	                  //static{} , {} 초기자
	public Lotto() { //특수한 목적 : member field 초기화 : (생성자 함수), constructor
		scanner = new Scanner(System.in); //초기화 (참조 변수는 주소값을 갖는 것) 
		r = new Random();
		numbers = new int[6];
	}
	
	//기능을 구현하세요
	//
	
	//r.nextInt(45)+1    >>   1~45
	
	
	
	
	
	
	
	
	
} // end Lotto
