package Study_class;

public class Fan {
	private String brand;
	private String model;
	private String color;
	public int speed;
	
	private static int price;
	
	Motor mo;
	
	public void powerOn() {
		System.out.println("전원이 켜졌습니다");
	}

	public void powerOff() {
		System.out.println("전원이 꺼졌습니다");
	}
	
	public void speedUp() {
		System.out.println("전원이 켜졌습니다");
	}

	public void speedDown() {
		System.out.println("전원이 꺼졌습니다");
	}
	
	    

}


