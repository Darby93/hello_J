
public class Lotto_02 {

	public static void main(String[] args) {
		int [] lotto = new int[] {7,3,27,19,5,14};
		
		
//		for (int i = 0; i < 6; i++) {
//			lotto[i] = (int)(Math.random()*45+1);
//			for (int j =  0; i > j ; j++) {
//				if (lotto[i] == lotto[j]) {
//					i--;
//					break;
//				}
//			}
//		}
		
		for (int i = 0; i < lotto.length; i ++) {
			for (int j = 0; j < lotto.length; j++) {
				if(lotto[i] < lotto[j]) {
					int temp = lotto[i];
					lotto[i] = lotto[j];
					lotto[j] = temp;
				}
			}
		}
		
		
		
		
		
		for ( int i = 0; i < 6; i++) {
			System.out.println(lotto[i]);
		}
		
	}

}

