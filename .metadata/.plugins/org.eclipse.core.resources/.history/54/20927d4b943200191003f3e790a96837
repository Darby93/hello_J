package home.or.bit;

import java.util.Random;

public class Lotto_home {
	private Random r;
	private int[] numbers;

	public void pick() {
		numbers = new int[6];
		r = new Random();
		for (int i = 0; i < numbers.length; i++) {
			numbers[i] = r.nextInt(45) + 1;
			
			for (int j = 0; j < i; j++) {
				if (numbers[i] == numbers[j]) {
					i--;
					break;
				}

			}
			
		}
		
		for(int i = 0; i < lotto.length; i++) {
			System.out.println(lotto[i]);
		}
	}
	

}
