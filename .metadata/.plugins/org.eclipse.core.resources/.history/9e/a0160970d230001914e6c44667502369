class Man{
	String name;
	int age;
}

//return Type 또는 parameter 자워으로 사용
//1. 8가지 기본타입
//2. String : String m(String str){return ...}
//3. Array : int [] m (int [] arr) {return ...}
//4. 사용자 정의 클래스(Type) Man m (Man m){}


class Test{
	Man add(Man m) {
		m.name = "누구";
		m.age = 10;
		
		return m;
	}
}



public class Ex12_Method_Overloading_Object {

	public static void main(String[] args) {
	}

}
