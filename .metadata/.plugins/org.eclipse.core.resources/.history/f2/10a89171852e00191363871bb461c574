//static method
class StaticClass{
	public int iv; //멤버필드
	static int cv; //멤버필드
	
	//일반함수
	void m () {
		//일반함수 m() 에서 iv 값을 처리 : (0)
		iv = 100;
		//일반함수 m() cv값을 처리 : (0)
		//static 자원은 일반자원보다 먼저(항상) memory에 로드 된다
		cv = 200;
		
		System.out.println("iv : " + iv + " , " + "cv : " + cv);
		
	}
	
	static void sm() {
		
	}
}

public class Ex07_Static_Method {

	public static void main(String[] args) {
		StaticClass s = new StaticClass();
		s.m();
		
		
	}

}
