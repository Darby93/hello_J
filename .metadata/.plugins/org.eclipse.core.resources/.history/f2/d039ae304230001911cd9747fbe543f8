

public class Lotto_03 {
	public static void main(String[] args) {
		
		int [] lotto = new int[6];
		
		for(int i = 0; i < lotto.length; i++) {
			int r = (int)(Math.random()*45+1);
			lotto[i] = r;
			for(int j = 0; j < i; j++) {
				if(lotto[i] == lotto[j]) {
					i--;
					break;
				}
			}
		}
		
		for(int i = 0; i < lotto.length; i++) {
			for(int j = 0; j < lotto.length; j++)
		}
		
			
		
		for(int value : lotto) {
			System.out.println(value);
		}
		
	}
	
}
