import java.util.Scanner;

public class Assignment_03 {

	public static void main(String[] args) {
//		Scanner sc = new Scanner(System.in);
//		System.out.println("숫자를 입력하세요");
//		int floor = sc.nextInt();
		
		for (int i = 6; i >= 1; i--) {
			for (int j = 1; j <= 4; j++) {
				if(i <= j) {
				System.out.print("*");
				}else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
