import kr.or.bit.Car;

public class Ex03_Modifier {
	public static void main(String[] args) {
		Car car = new Car();
		
		car.setWindow(10);
		System.out.println(car.getWindow());
		
		car.setSpeed(-39);
		System.out.println(car.getSpeed());
		
		System.out.println("현재 시스템 속도 : " + car.getSpeed());
		car.speedUp();
	}

}
